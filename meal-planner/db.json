    {
      "title": "Paella",
      "image": "/assets/images/paella.jpeg",
      "protein": "chicken chorizo shrimp mussels",
      "season": "summer fall",
      "ethnicity": "Spanish",
      "count": 6,
      "cook_time": "20 mins",
      "prep_time": "10 mins",
      "servings": 4,
      "isAddedToMenu": false,
      "breakfast": " ",
      "grocery_items": [
        {
          "ingredient": "fresh parsley",
          "amount": "1 cup"
        },
        {
          "ingredient": "lemon juice",
          "amount": "1/4 cup"
        },
        {
          "ingredient": "olive oil",
          "amount": "1 tbsp"
        },
        {
          "ingredient": "garlic",
          "amount": "2 cloves"
        },
        {
          "ingredient": "saffron",
          "amount": "1 tsp"
        },
        {
          "ingredient": "chicken broth",
          "amount": "48 oz"
        },
        {
          "ingredient": "jumbo shrimp",
          "amount": "8"
        },
        {
          "ingredient": "chicken thighs",
          "amount": "4"
        },
        {
          "ingredient": "chorizo",
          "amount": "2 links"
        },
        {
          "ingredient": "iberico ham",
          "amount": "4 oz"
        },
        {
          "ingredient": "onions",
          "amount": "2 cups"
        },
        {
          "ingredient": "Arborio rice",
          "amount": "3 cups"
        },
        {
          "ingredient": "mussels",
          "amount": "8"
        },
        {
          "ingredient": "smoked paprika",
          "amount": "1 tsp"
        }
      ],
      "directions": [
        {
          "step": "To prepare the herb blend, combine the first 4 ingredients, and set aside."
        },
        {
          "step": "To prepare paella, combine water, saffron, and broth in a large saucepan. Bring to a simmer (do not boil). Keep warm over low heat."
        },
        {
          "step": "Peel and devein shrimp, leaving tails intact; set aside."
        },
        {
          "step": "Heat 1 tablespoon oil in a large paella pan or large skillet over medium-high heat. Add chicken; saute 2 minutes on each side. Remove from pan."
        },
        {
          "step": "Add sausage and prosciutto; saute 2 minutes. Remove from pan."
        },
        {
          "step": "Add shrimp, and saute 2 minutes. Remove from pan. Reduce heat to medium-low."
        },
        {
          "step": "Add onion and bell pepper; saute 15 minutes, stirring occasionally. Add tomatoes, paprika, and 3 garlic cloves; cook 5 minutes."
        },
        {
          "step": "Add rice; cook 1 minute, stirring constantly. Stir in herb blend, broth mixture, chicken, sausage mixture, and peas. Bring to a low boil; cook 10 minutes, stirring frequently."
        },
        {
          "step": "Add mussels to pan, nestling them into rice mixture. Cook 5 minutes or until shells open; discard any unopened shells."
        },
        {
          "step": "Arrange shrimp, heads down, in rice mixture, and cook 5 minutes or until shrimp are done. Sprinkle with 1/4 cup lemon juice. Remove from heat; cover with a towel, and let stand 10 minutes. Serve with lemon wedges, if desired."
        }
      ],
